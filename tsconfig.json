{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.js", "**/*.js", "**/*.jsx", "redux/cart.slice.js", "redux/store.js", "pages/cart.jsx", "pages/index.jsx", "pages/[id].jsx", "pages/_app.jsx", "pages/products/index.jsx", "services/productServices.js", "components/header.jsx", "components/layout.jsx", "components/footer.jsx", "components/CartItem.jsx"],
  "exclude": ["node_modules"]
}
